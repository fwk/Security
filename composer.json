{
    "name": "fwk/security",
    "description": "Fwk Authorization and Authentication based on ZF2",
    "type": "library",
    "keywords": ["library", "auth", "authorization", "authentication"],
    "license": "BSD",
    "authors": [
        {
            "name": "neiluJ",
            "email": "julien@nitronet.org"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "fwk/events": "dev-master",
        "fwk/xml": "dev-master",
        "zendframework/zend-authentication": "2.*",
        "zendframework/zend-permissions-acl": "2.*",
        "zendframework/zend-crypt": "2.*"
    },
    "require-dev": {
        "symfony/http-foundation": "dev-master",
        "zendframework/zend-http": "2.*",
        "fwk/db": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Fwk\\Security": ""
        }
    },
    "minimum-stability": "dev",
    "target-dir": "Fwk/Security"
}
